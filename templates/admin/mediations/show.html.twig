{% extends 'base.html.twig' %}

{% block title %}Mediations{% endblock %}

{% block body %}
<div class="administration">
    <h1>Mediations</h1>
    <fieldset class="container">
        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ mediation.id }}</td>
                </tr>
                <tr>
                    <th>Mediation_history</th>
                    <td>{{ mediation.mediationHistory }}</td>
                </tr>
                <tr>
                    <th>Mediation_pedagogy</th>
                    <td>{{ mediation.mediationPedagogy }}</td>
                </tr>
                <tr>
                    <th>Mediation_definition</th>
                    <td>{{ mediation.mediationDefinition }}</td>
                </tr>
            </tbody>
        </table>

        <a href="{{ path('mediations_index') }}">back to list</a>

        <a href="{{ path('mediations_edit', {'id': mediation.id}) }}">edit</a>

        {{ include('mediations/_delete_form.html.twig') }}
    </fieldset>
</div>
{% endblock %}
