{% extends 'admin.html.twig' %}

{% block title %}AnimalsCategories index{% endblock %}

{% block body %}
    <h1>AnimalsCategories index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Animal_category_name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for animals_category in animals_categories %}
            <tr>
                <td>{{ animals_category.id }}</td>
                <td>{{ animals_category.animalCategoryName }}</td>
                <td>
                    <a href="{{ path('admin_animals_categories_show', {'id': animals_category.id}) }}">show</a>
                    <a href="{{ path('admin_animals_categories_edit', {'id': animals_category.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_animals_categories_new') }}">Create new</a>
{% endblock %}
