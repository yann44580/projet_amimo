{% extends 'base.html.twig' %}

{% block title %}PopulationsType index{% endblock %}

{% block body %}
    <h1>PopulationsType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Population_type_name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for populations_type in populations_types %}
            <tr>
                <td>{{ populations_type.id }}</td>
                <td>{{ populations_type.populationTypeName }}</td>
                <td>
                    <a href="{{ path('admin_populations_type_show', {'id': populations_type.id}) }}">show</a>
                    <a href="{{ path('admin_populations_type_edit', {'id': populations_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_populations_type_new') }}">Create new</a>
{% endblock %}
