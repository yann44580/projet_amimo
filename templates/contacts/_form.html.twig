{{ form_start(form) }}
<div class="input-field">
	{{ form_label(form.contact_fullname) }}
	{{ form_widget(form.contact_fullname) }}
</div>
<div class="input-field">
	{{ form_label(form.contact_firstname) }}
	{{ form_widget(form.contact_firstname) }}
</div>
<div class="input-field">
	{{ form_label(form.contact_email) }}
	{{ form_widget(form.contact_email) }}
</div>
<div class="input-field">
	{{ form_label(form.contact_subject) }}
	{{ form_widget(form.contact_subject) }}
</div>
<div class="input">
	{{ form_label(form.contact_content) }}
	{{ form_widget(form.contact_content) }}
</div>
<div class="button">
	<button class="btn">{{ button_label|default('Envoyer') }}</button>
</div>
{{ form_end(form) }}
